{
  "manifest_version": 3,
  "name": "Jut.su Extended",
  "version": "1.1",
  "description": "Automatically skips the opening and ending of the series on the site jut.su",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "/icon/1.png"
  },
  "content_scripts": [
    {
      "matches": ["https://jut.su/*"],
      "js": ["/main.js"]
    }
  ]
}
