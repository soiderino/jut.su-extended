{
  "manifest_version": 3,
  "name": "Jut.su Extended",
  "version": "1.0",
  "description": "Automatically skips opening/ending in Jut.su",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "/icon/1.png"
  },
  "content_scripts": [
    {
      "matches": ["https://jut.su/*"],
      "js": ["/main.js"]
    }
  ]
}
